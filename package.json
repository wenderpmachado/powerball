{
  "name": "powerball",
  "version": "0.1.0",
  "description": "A challenge made with typescript and best practices",
  "author": "Wender Machado <wenderpmachado@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/wenderpmachado/powerball.git"
  },
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/index",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 app/index",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "format": "prettier --write \"**/*.{js,json,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,yaml,yml,md}\""
  },
  "dependencies": {
    "axios": "^0.23.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "multer": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^7.1.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
